name="gdbm"
version="1.8.3"
source="http://mirrors.kernel.org/gnu/$name/$name-$version.tar.gz"
packager="custom_package"
verify="cc340338a2e28b40058ab9eb5354a21d53f88a1582ea21ba0bb185c37a281dc9"

custom_package() {
	make prefix=${DESTDIR}/usr \
		manprefix=${DESTDIR}/usr/share/man \
		man3dir=${DESTDIR}/usr/share/man/man3 \
		infodir=${DESTDIR}/usr/share/info \
		install
}
