name="glibc"
fullname="glibc-2.14"
version="2.14"
source="http://mirrors.kernel.org/gnu/glibc/glibc-2.14.tar.bz2"
build_args="--disable-profile --enable-add-ons \
--enable-kernel=2.6.22.5 --libexecdir=/usr/lib/glibc"
verify="gnu_verify"

custom_install() {
	unset MAKEFLAGS
	make install_root=$DESTDIR install
}
