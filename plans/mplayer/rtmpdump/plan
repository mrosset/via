name="rtmpdump"
version="2.3"
source="http://rtmpdump.mplayerhq.hu/download/rtmpdump-2.3.tgz"
build_name=""
build="gnu_make"
build_args="prefix=/usr"
install="custom_install"
install_args="prefix=/usr"
verify="sha_verify"
verify_args="ef38b7a99d82ce6912063d21063aeaf28185341b3df486e24bffce5354224b2c"

custom_install() {
	mkdir -p $DESTDIR
	gnu_install
}
