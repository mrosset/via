name="rxvt-unicode"
version="9.12"
source="http://dist.schmorp.de/rxvt-unicode/rxvt-unicode-9.12.tar.bz2"
build_args="--enable-everything --enable-256-color"
verify="1db334204bfb264b3134e01d9d3cf446c5ac7d47514909435f914f3906f37e7e"

custom_install() {
	install -d "$DESTDIR/usr/share/terminfo"
	export TERMINFO="$DESTDIR/usr/share/terminfo"
	gnu_package
}
