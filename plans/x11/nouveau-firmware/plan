name="nouveau-firmware"
version="20091212"
source="http://people.freedesktop.org/~pq/nouveau-drm/nouveau-firmware-20091212.tar.gz"
build_name="nouveau"
builder="no_build"
verify="4eaeb2044c1da95b50ea4f1facb0effc74d65d8b13dc10893c26e4fe9fe23fb8"

custom_install() {
	for i in *; do
		install -Dm 644 $i $DESTDIR/lib/firmware/nouveau/$i
	done
}
