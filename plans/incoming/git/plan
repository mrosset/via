name="git"
version="1.7.6"
source="http://kernel.org/pub/software/scm/git/git-1.7.6.tar.bz2"
builder="relocate_build"
build_args="--without-python"
verify="778795cece63cd758192378f3a999870cea290181b3a4c9de573c77192561082"

post_hook() {
	install -Dm 644 $builds/$build_name/contrib/completion/git-completion.bash $DESTDIR/etc/bash_completion.d/git
}
