name="gdbm"
version="1.8.3"
source="http://mirrors.kernel.org/gnu/$name/$name-$version.tar.gz"
build_name=""
pre=""
build="gnu_build"
build_args=""
install="custom_install"
install_args=""
post=""
verify="sha_verify"
verify_args="cc340338a2e28b40058ab9eb5354a21d53f88a1582ea21ba0bb185c37a281dc9"

custom_install() {
	fakeroot make prefix=${DESTDIR}/usr \
		manprefix=${DESTDIR}/usr/share/man \
		man3dir=${DESTDIR}/usr/share/man/man3 \
		infodir=${DESTDIR}/usr/share/info \
		install
}
